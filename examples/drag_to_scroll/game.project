[project]
title = Drag to Scroll
custom_resources = 

[bootstrap]
main_collection = /drag_to_scroll/drag_to_scroll.collectionc
render = /builtins/render/default.renderc

[input]
game_binding = /input/game.input_bindingc
gamepads = /builtins/input/default.gamepadsc

[display]
width = 1136
height = 640
variable_dt = 1

[physics]
scale = 0.02
debug = 0
max_collisions = 256
max_contacts = 256
gravity_y = -100

[script]
shared_state = 1

[android]
iap_provider = GooglePlay
package = com.britzl.publicexamples.drag_to_scroll

[sprite]
max_count = 1000

[resource]
max_resources = 1000

[osx]
bundle_identifier = com.britzl.publicexamples.drag_to_scroll

[ios]
bundle_identifier = com.britzl.publicexamples.drag_to_scroll

[tilemap]
max_tile_count = 9000

