[project]
title = Top Down Movement
custom_resources = 
dependencies#0 = https://github.com/defold/defold-examples/releases/download/0.2.0/war-battles.zip

[bootstrap]
main_collection = /top_down_movement/top_down_movement.collectionc
render = /builtins/render/default.renderc

[input]
game_binding = /input/game.input_bindingc
gamepads = /builtins/input/default.gamepadsc

[display]
width = 640
height = 1136
variable_dt = 1

[physics]
scale = 0.02
debug = 0
max_collisions = 256
max_contacts = 256
gravity_y = -100

[script]
shared_state = 1

[android]
iap_provider = GooglePlay
package = com.britzl.publicexamples.top_down_movement

[osx]
bundle_identifier = com.britzl.publicexamples.top_down_movement

[ios]
bundle_identifier = com.britzl.publicexamples.top_down_movement

[tilemap]
max_tile_count = 5700

