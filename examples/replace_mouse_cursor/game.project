[project]
title = Replace Mouse Cursor
custom_resources = 

[bootstrap]
main_collection = /replace_mouse_cursor/replace_mouse_cursor.collectionc
render = /builtins/render/default.renderc

[input]
game_binding = /input/game.input_bindingc
gamepads = /builtins/input/default.gamepadsc

[display]
width = 640
height = 1136
variable_dt = 1

[physics]
scale = 0.02
debug = 0
max_collisions = 256
max_contacts = 256
gravity_y = -100

[script]
shared_state = 1

[android]
iap_provider = GooglePlay
package = com.britzl.publicexamples.replace_mouse_cursor

[osx]
bundle_identifier = com.britzl.publicexamples.replace_mouse_cursor

[ios]
bundle_identifier = com.britzl.publicexamples.replace_mouse_cursor

[library]
include_dirs = https://github.com/subsoap/defos/archive/refs/tags/v2.7.1.zip

